<h3 class="filter-header">Filter By Room</h3>
<h3 class="clear-btn">Clear</h3>
<span role="button" class="close-filter">&#x2715;</span>
<hr class="hr">
<div class="room-filters">
    <ul class="room-filter-list">
        {{#each roomTypes}}
            <li class="room-list-item" role="button" data="{{this}}">{{this}}</li>
        {{/each}}
    </ul>
    <hr class="hr">
</div>